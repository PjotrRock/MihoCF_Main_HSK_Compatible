<ThingDef ParentName="ApparelMakeableBase">
    <defName>Miho_Apparel_Shell_Sorceress</defName>
    <label>미호 소서리스 망토</label>
    <description>체내 메카나이트들을 활발하게 만들어주는 고급 망토입니다.</description>
    <graphicData>
      <texPath>Miho/Apparel/ShellSorceress/ShellSorceress</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>75</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <costList>
      <Miho_Ebony_SilkCloth>150</Miho_Ebony_SilkCloth>
      <ComponentSpacer>4</ComponentSpacer>
    </costList>
    <thingCategories>
      <li>Miho_Apparel</li>
    </thingCategories>
    <recipeMaker>
      <researchPrerequisite>Miho_ApparelSorcerees</researchPrerequisite>

      <recipeUsers Inherit="false">
        <li>Miho_TableMachining</li>
      </recipeUsers>
    </recipeMaker>
    <statBases>
	  <MaxHitPoints>300</MaxHitPoints>
	  <WorkToMake>115000</WorkToMake>
      <Mass>3</Mass>
      <StuffEffectMultiplierArmor>0.15</StuffEffectMultiplierArmor>
      <ArmorRating_Sharp>0.15</ArmorRating_Sharp>
	  <ArmorRating_Heat>0.4</ArmorRating_Heat>
      <EquipDelay>5</EquipDelay>
      <StuffEffectMultiplierInsulation_Cold>0.75</StuffEffectMultiplierInsulation_Cold>
	  <Insulation_Cold>7</Insulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <NegotiationAbility>0.05</NegotiationAbility>
      <SocialImpact>0.05</SocialImpact>
      <PsychicEntropyRecoveryRate>1.5</PsychicEntropyRecoveryRate>
      <PsychicEntropyMax>120</PsychicEntropyMax>
      <MoveSpeed>-0.12</MoveSpeed>

    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
		<li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Miho/Apparel/ShellSorceress/ShellSorceress</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
        <li>MihoRoyal</li>
        <li>Royal</li>
		</tags>
      <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>

      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(255,255,255)</only>
        </li>
        <li>
          <weight>15</weight>
          <only>(255,255,255)</only>
        </li>
        <li>
          <weight>20</weight>
          <only>(255,255,255)</only>
        </li>
      </options>
    </colorGenerator>

  </ThingDef>
  	

</Defs>

  <HediffDef>
    <defName>Miho_Plasma_Hediff</defName>
    <label>갑옷 융해</label>
    <description>갑옷이 열에 의해 부분적으로 액체화 되었습니다.</description>
    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(0.7, 1.0, 0.7)</defaultLabelColor>
    <scenarioCanAdd>true</scenarioCanAdd>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-72</severityPerDay>
        <showHoursToRecover>true</showHoursToRecover>
      </li>
	  <li Class="HediffCompProperties_Effecter">
				<stateEffecter>Miho_Melt</stateEffecter>
	  </li>
    </comps>
    <stages>
      <li>
        <label>initial</label>
        <becomeVisible>false</becomeVisible>
      </li>
      <li>
        <label>initial</label>
        <minSeverity>0.04</minSeverity>
        <statOffsets>
         <ArmorRating_Sharp>-0.2</ArmorRating_Sharp>
	     <ArmorRating_Blunt>-0.1</ArmorRating_Blunt>
     	</statOffsets>
      </li>
      <li>
        <label>minor melted</label>
        <minSeverity>0.2</minSeverity>
        <statOffsets>
         <ArmorRating_Sharp>-0.3</ArmorRating_Sharp>
	     <ArmorRating_Blunt>-0.2</ArmorRating_Blunt>
         <IncomingDamageFactor>0.1</IncomingDamageFactor>

     	</statOffsets>
      </li>
      <li>
        <label>moderate melted</label>
        <minSeverity>0.40</minSeverity>
        <statOffsets>
         <ArmorRating_Sharp>-0.4</ArmorRating_Sharp>
	     <ArmorRating_Blunt>-0.3</ArmorRating_Blunt>
         <IncomingDamageFactor>0.2</IncomingDamageFactor>

     	</statOffsets>
      </li>
      <li>
        <label>severe melted</label>
        <minSeverity>0.60</minSeverity>
        <statOffsets>
         <ArmorRating_Sharp>-0.5</ArmorRating_Sharp>
	     <ArmorRating_Blunt>-0.4</ArmorRating_Blunt>
         <IncomingDamageFactor>0.3</IncomingDamageFactor>
     	</statOffsets>
      </li>
      <li>
        <label>totally melted</label>
        <minSeverity>0.95</minSeverity>
        <statOffsets>
         <ArmorRating_Sharp>-0.75</ArmorRating_Sharp>
	     <ArmorRating_Blunt>-0.5</ArmorRating_Blunt>
         <IncomingDamageFactor>3</IncomingDamageFactor>
     	</statOffsets>
      </li>
    </stages>
  </HediffDef>

